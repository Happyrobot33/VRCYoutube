{
  "name": "Happyrobot33s Youtube Questifyier",
  "version": "1.6",
  "description": "A tool to convert youtube links to work on quest in VRC!",
  "manifest_version": 3,
  "author": "Happyrobot33",
  "permissions": ["clipboardWrite", "activeTab", "storage"],
  "host_permissions": [],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "VRCYoutube"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "matthewherber@sbcglobal.net"
    }
  }
}
